{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
    ],
    "functions": {
        "yourFunctionName": {
          "memory": 512,
          "maxDuration": 30 
      }
  }
}
